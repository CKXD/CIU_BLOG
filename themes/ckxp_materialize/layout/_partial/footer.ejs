  <script>

      document.addEventListener('DOMContentLoaded', function() {
          var sideN = document.querySelectorAll('.sidenav');
          var instances = M.Sidenav.init(sideN);
        });

      document.addEventListener('DOMContentLoaded', function() {
          var dropd = document.querySelectorAll('.dropdown-trigger');
