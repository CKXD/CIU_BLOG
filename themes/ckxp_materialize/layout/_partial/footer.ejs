      document.addEventListener('DOMContentLoaded', function() {
          var ecaro = document.querySelectorAll('.carousel');
          var instances = M.Carousel.init(ecaro, {'duration' : 100, 'numVisible' : 5, 'indicators' : false, 'noWrap' : false});
        });

